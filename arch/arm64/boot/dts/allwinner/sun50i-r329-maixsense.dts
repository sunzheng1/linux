&codec {
	status = "disable";
	allwinner,audio-routing =
		"Speaker", "SPKL",
		"MIC1", "Mic",
		"MIC2", "Mic",
		"Mic", "MBIAS",
		"Mic", "MBIAS";
};

&codec_analog {
	status = "disable";
};

&ehci0 {
	status = "okay";
};

&lradc {
	status = "disable";

	button-155 {
		label = "Right";
		linux,code = <KEY_RIGHT>;
		channel = <0>;
		voltage = <155013>;
	};

	button-310 {
		label = "Left";
		linux,code = <KEY_LEFT>;
		channel = <0>;
		voltage = <309828>;
	};

	button-463 {
		label = "B";
		linux,code = <KEY_B>;
		channel = <0>;
		voltage = <463317>;
	};

	button-599 {
		label = "A";
		linux,code = <KEY_A>;
		channel = <0>;
		voltage = <599420>;
	};
};

&mmc0 {
	pinctrl-names = "default";
	pinctrl-0 = <&mmc0_pf_pins>;

	vmmc-supply = <&reg_vcc3v3>;
	bus-width = <4>;
	cd-gpios = <&pio 5 6 GPIO_ACTIVE_LOW>; /* PF6 */
	status = "okay";
};

&ohci0 {
	status = "okay";
};

&pwm {
	pinctrl-names = "default";
	pinctrl-0 = <&pwm2_pb2>, <&pwm3_pb3>, <&pwm7_pb7>, <&pwm8_pb8>;
	status = "okay";
};

