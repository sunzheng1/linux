&r_i2c {
	status = "okay";
};

&spi1 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi1_clk_ph1>, <&spi1_mosi_ph2>, <&spi1_miso_ph3>;
	//<&spi1_cs_ph0>, 
	status = "okay";

	cs-gpios = <&pio 7 0 GPIO_ACTIVE_HIGH>,<&r_pio 0 2 GPIO_ACTIVE_HIGH>,<&r_pio 0 4 GPIO_ACTIVE_HIGH>,<&r_pio 0 3 GPIO_ACTIVE_HIGH>;
	num-cs = <3>;
	spidev@0 {
		status = "okay";

		compatible = "spidev";
		/* spi default max clock 100Mhz */
		spi-max-frequency = <100000000>;
		reg = <0>;
	};

	spidev@1 {
		status = "okay";

		compatible = "spidev";
		/* spi default max clock 100Mhz */
		spi-max-frequency = <100000000>;
		reg = <1>;
	};
	spidev@2 {
		status = "okay";

		compatible = "spidev";
		/* spi default max clock 100Mhz */
		spi-max-frequency = <100000000>;
		reg = <2>;
	};
    spidev@3 {
		status = "okay";

		compatible = "spidev";
		/* spi default max clock 100Mhz */
		spi-max-frequency = <100000000>;
		reg = <3>;
	};
	// lcd: lcd@0 {
